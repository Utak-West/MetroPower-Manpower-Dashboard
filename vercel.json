    "REQUEST_TIMEOUT": "30000",
    "FEATURE_REALTIME_UPDATES": "true",
    "FEATURE_EMAIL_NOTIFICATIONS": "false",
    "FEATURE_FILE_EXPORTS": "true",
    "FEATURE_USER_MANAGEMENT": "false",
    "FEATURE_AUDIT_LOGGING": "true",
    "CORS_ORIGIN": "*",
    "VERCEL": "1",
    "VERCEL_PROTECTION_BYPASS": "true"
  },
  "regions": ["iad1"],
  "github": {
    "silent": true
  },
  "public": true
}
